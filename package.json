    "test": "npm run test:s && npm run test:t",
    "test:ci": "mocha --require ts-node/register schematics/**/*.spec.ts && mocha --require ts-node/register src/**/*.spec.ts"
  },
  "builders": "builders.json",
  "schematics": "schematics/collection.json",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.19",
    "@types/chai": "^4.2.5",
