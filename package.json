    "dedent": "^0.7.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "mocha-logger": "^1.0.6",
    "prettier": "^1.19.1",
    "rxjs": "^6.5.3",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": ">=0.800.0",
    "rxjs": ">= 6.0.0"
  },
